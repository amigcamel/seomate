{
  "name": "seomate",
  "version": "0.0.1",
  "description": "An NPM module providing dead simple way to check your HTML defects",
  "repository": {
    "type": "git",
    "url": "https://github.com/amigcamel/seomate"
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha || true",
    "lint": "eslint index.js lib test",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "keywords": [
    "SEO"
  ],
  "bin": {
    "seomate": "./lib/cli.js"
  },
  "author": "amigcamel",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "rewire": "^4.0.1",
    "should": "^13.2.1",
    "sinon": "^6.1.3"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.16.0",
    "get-stream": "^3.0.0",
    "is-valid-path": "^0.1.1"
  }
}
